{
  "name": "Hostelworld on Roids",
  "version": "0.0.0",
  "description": "This extension is made for enhancing your hostelworld experience.",
  "manifest_version": 3,
  "permissions": [
    "scripting"
  ],
  "content_scripts": [
    {
      "js": ["content.js"],
      "run_at": "document_start",
      "matches": ["https://*.hostelworld.com/*"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["roids.js"],
      "matches": ["https://*.hostelworld.com/*"]
    }
  ]
}
