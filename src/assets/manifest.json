{
  "name": "Hostelworld on Roids",
  "version": "0.0.0",
  "description": "This extension is made for enhancing your hostelworld experience.",
  "manifest_version": 3,
  "permissions": [
    "scripting"
  ],
  "content_scripts": [
    {
      "matches": ["https://*.hostelworld.com/*"],
      "run_at": "document_end",
      "js": ["content.js"]
    }
  ]
}