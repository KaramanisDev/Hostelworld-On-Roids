{
  "name": "Hostelworld on Roids",
  "version": "0.0.0",
  "description": "This extension is made for enhancing your hostelworld experience.",
  "manifest_version": 3,
  "content_scripts": [
    {
      "js": ["content.js"],
      "css": ["roids.css"],
      "run_at": "document_start",
      "matches": ["https://hostelworld.com/*", "https://www.hostelworld.com/*"]
    }
  ],
  "background": {
    "service_worker": "worker.js",
    "type": "module"
  },
  "web_accessible_resources": [
    {
      "resources": ["roids.js"],
      "matches": ["https://hostelworld.com/*", "https://www.hostelworld.com/*"]
    }
  ]
}
